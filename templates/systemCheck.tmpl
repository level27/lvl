    {{ get $.CheckParametersDescriptions $key }}
    {{ $key }}: {{vt "brightwhite"}}{{ $dat.Value }}    
  {{ end }}{{ end }}{{if not $anyParams}}{{vt "brightblack"}}(none){{ end -}}

{{- /* PARAMETERS */}}
  {{- $anyParams := false }}
  Alerts: {{ range $key, $dat := .Alerts }}{{ if not $dat.Default }}
    {{- $anyParams = true }}
    {{ $key }}: {{vt "brightwhite"}}{{ $dat.Value }}
    {{ end }}{{ end }}{{if not $anyParams}}{{vt "brightblack"}}(none){{ end -}}